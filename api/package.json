{
  "name": "rezeptor",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "predev": "npm run clean && npm run build",
    "dev": "concurrently --raw \"tsc -w\" \"func start\"",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "predeploy": "npm prune --omit=dev",
    "deploy": "func azure functionapp publish rezeptor",
    "postdeploy": "npm install",
    "lint": "eslint ."
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "5.0.0",
    "@azure/cosmos": "4.0.0",
    "@azure/functions": "4.3.0",
    "@azure/identity": "4.0.1",
    "@azure/storage-blob": "12.17.0",
    "@googleapis/books": "1.0.5",
    "@simplewebauthn/server": "9.0.3",
    "applicationinsights": "2.9.5",
    "capitalize": "2.0.4",
    "cookie": "0.6.0",
    "iti": "0.6.0",
    "sharp": "0.33.2",
    "stopword": "3.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "9.1.1",
    "@types/capitalize": "2.0.2",
    "@types/cookie": "0.6.0",
    "@types/node": "20.11.27",
    "@types/stopword": "2.0.3",
    "azure-functions-core-tools": "4.0.5530",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-unused-imports": "3.1.0",
    "rimraf": "5.0.5",
    "typescript": "5.4.2",
    "typescript-eslint": "7.7.1"
  },
  "main": "dist/src/**/functions/*.js"
}