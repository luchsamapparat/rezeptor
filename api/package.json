{
  "name": "rezeptor",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "predev": "npm run clean && npm run build",
    "dev": "concurrently --raw \"tsc -w\" \"func start\"",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "predeploy": "npm prune --omit=dev",
    "deploy": "func azure functionapp publish rezeptor",
    "postdeploy": "npm install",
    "lint": "eslint ."
  },
  "dependencies": {
    "@azure/ai-form-recognizer": "5.0.0",
    "@azure/cosmos": "4.1.1",
    "@azure/functions": "4.6.0",
    "@azure/identity": "4.5.0",
    "@azure/storage-blob": "12.25.0",
    "@googleapis/books": "4.0.0",
    "@simplewebauthn/server": "10.0.1",
    "applicationinsights": "2.9.6",
    "capitalize": "2.0.4",
    "cookie": "1.0.1",
    "iti": "0.7.0",
    "sharp": "0.33.5",
    "stopword": "3.1.1",
    "zod": "3.23.8",
    "zod-form-data": "2.0.2"
  },
  "devDependencies": {
    "@eslint/js": "9.15.0",
    "@types/capitalize": "2.0.2",
    "@types/node": "22.9.0",
    "@types/stopword": "2.0.3",
    "azure-functions-core-tools": "4.0.6610",
    "concurrently": "9.1.0",
    "eslint": "8.57.1",
    "eslint-plugin-export-scope": "2.4.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "rimraf": "5.0.10",
    "typescript": "5.6.3",
    "typescript-eslint": "7.18.0"
  },
  "main": "dist/src/**/functions/*.js"
}