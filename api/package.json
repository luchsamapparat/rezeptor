    "@azure/storage-blob": "12.17.0",
    "@googleapis/books": "1.0.5",
    "capitalize": "2.0.4",
    "sharp": "0.33.2",
    "stopword": "3.0.1",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/capitalize": "2.0.2",
    "@types/node": "^18.x",
    "@types/stopword": "2.0.3",
    "azure-functions-core-tools": "4.0.5530",
    "azurite": "3.29.0",
    "rimraf": "^5.0.0",
    "typescript": "^4.0.0"
  },
  "main": "dist/src/functions/*.js"
