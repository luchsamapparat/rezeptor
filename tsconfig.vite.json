{
  "extends": "./tsconfig.json",
  "include": [
    ".react-router/types/**/*",
    "src/**/*",
  ],
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "types": [
      "vite/client"
    ],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "rootDirs": [
      ".",
      "./.react-router/types"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}